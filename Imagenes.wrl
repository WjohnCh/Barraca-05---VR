#VRML V2.0 utf8
PROTO A4[
	field SFVec3f posicion 0 0 0
	field SFVec3f size 0 0 0
	field SFRotation rotacion 0 0 0 0
	field MFString imagen ["imagen.jpg"]
]{
	Transform{
		translation	IS posicion
		rotation IS	rotacion
		children[ Shape	{
			geometry Box{size IS size}
			appearance Appearance {
				texture	ImageTexture {url IS imagen}
			}
		}
		]

	}
	
}


